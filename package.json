{
	"name": "converter",
	"version": "1.1.0",
	"description": "Based on ImageMagick and ExifTool applications image converter",
	"main": "index.js",
	"module": "index.ts",
	"scripts": {
		"build": "tsc",
		"start": "npm run server:js",
		"server:js": "node build/index.js",
		"server:watch": "nodemon -e js -w ./build -x npm run server:js",
		"server:ts": "ts-node source/index.ts",
		"server:ts:watch": "nodemon -e ts -w ./source -x npm run server:ts",
		"test": "mocha -r ts-node/register source/**/*test.ts"
	},
	"dependencies": {
		"@types/node": "^12.12.54",
		"imagemagick-cli-wrapper": "git+https://github.com/a-chepugov/imagemagick-cli-wrapper.git#4dfb6663950da47d36573fa1f5d190295bcdc6a5",
		"typescript": "^3.9.7"
	},
	"devDependencies": {
		"@types/chai": "^4.2.12",
		"@types/mocha": "^5.2.7",
		"chai": "^4.2.0",
		"mocha": "^6.2.3",
		"nodemon": "^2.0.4",
		"ts-node": "^8.10.2",
		"typedoc": "^0.17.8"
	},
	"config": {
		"unsafe-perm": true
	},
	"repository": {
		"type": "git",
		"url": "https://gitlab.decluster.ria.com/a.chepugov/converter.git"
	},
	"keywords": [
		"converter",
		"imagemagick",
		"exiftool"
	],
	"author": "",
	"license": "ISC"
}
